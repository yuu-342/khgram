<%= form_for @user, url: sign_up_path do |f| %>
  <%#= form_error(@user) %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.text_field :password %>
  <%= f.label :password_confirmation %>
  <%= f.text_field :password_confirmation %>

  <%= f.submit '登録' %>
<% end %>